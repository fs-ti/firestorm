{
   "floater": {
      "@name": "script_recover",
      "@title": {
         "string": "Script Recovery"
      },
      "text:txt2": {
         "@name": "txt2",
         "#text": {
            "string": "\n      [APP_NAME] has detected recoverable scripts that may have been left over from a recent viewer crash.\n  \nIf you choose to recover these scripts they will be placed in the 'Lost and Found' folder of your inventory.\n  "
         }
      },
      "scroll_list:script_list": {
         "@name": "script_list",
         "scroll_list.columns:script_check": {
            "@label": {
               "string": ""
            },
            "@name": "script_check"
         },
         "scroll_list.columns:script_name": {
            "@label": {
               "string": "Script Name"
            },
            "@name": "script_name"
         }
      },
      "button:recover_btn": {
         "@label": {
            "string": "Recover"
         },
         "@name": "recover_btn"
      },
      "button:cancel_btn": {
         "@label": {
            "string": "Cancel"
         },
         "@name": "cancel_btn"
      }
   }
}