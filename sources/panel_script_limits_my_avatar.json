{
   "panel": {
      "@label": {
         "string": "Avatar"
      },
      "@name": "script_limits_my_avatar_panel",
      "text:script_memory": {
         "@name": "script_memory",
         "#text": {
            "string": "\n    Avatar Script Usage\n  "
         }
      },
      "text:memory_used": {
         "@name": "memory_used"
      },
      "text:urls_used": {
         "@name": "urls_used"
      },
      "text:loading_text": {
         "@name": "loading_text",
