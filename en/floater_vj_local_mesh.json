            },
            "view_border:local_mesh_settings_tab_border": {
               "@name": "local_mesh_settings_tab_border"
            },
            "check_box:local_mesh_scale_use_meters": {
               "@name": "local_mesh_scale_use_meters",
               "@label": {
                  "string": "Assume scale is in meters."
               },
               "@tool_tip": {
                  "string": "Ignore the cm scale units used by tools such as Maya"
               }
            },
            "text:lod_suffix_label": {
               "@name": "lod_suffix_label",
