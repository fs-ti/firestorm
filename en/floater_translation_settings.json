{
   "floater": {
      "@name": "floater_translation_settings",
      "@title": {
         "string": "Chat Translation Settings"
      },
      "str_ing:azure_api_key_not_verified": {
         "@name": "azure_api_key_not_verified",
         "#text": {
            "string": "Azure service identifier not verified. Status: [STATUS]. Please check your settings and try again."
         }
      },
      "str_ing:google_api_key_not_verified": {
         "@name": "google_api_key_not_verified",
         "#text": {
            "string": "Google API key not verified. Status: [STATUS]. Please check your key and try again."
         }
      },
      "str_ing:deepl_api_key_not_verified": {
         "@name": "deepl_api_key_not_verified",
         "#text": {
            "string": "DeepL Auth Key key not verified. Status: [STATUS]. Please check your key and try again."
         }
      },
      "str_ing:azure_api_key_verified": {
         "@name": "azure_api_key_verified",
         "#text": {
            "string": "Azure service identifier verified."
         }
      },
      "str_ing:google_api_key_verified": {
         "@name": "google_api_key_verified",
         "#text": {
            "string": "Google API key verified."
         }
      },
      "str_ing:deepl_api_key_verified": {
         "@name": "deepl_api_key_verified",
         "#text": {
            "string": "DeepL API key verified."
         }
      },
      "check_box:translate_chat_checkbox": {
         "@label": {
            "string": "Enable machine translation while chatting"
         },
         "@name": "translate_chat_checkbox"
      },
      "text:translate_language_label": {
         "@name": "translate_language_label",
         "#text": {
            "string": "\n  Translate chat into: \n "
         }
      },
      "combo_box:translate_language_combo": {
         "@name": "translate_language_combo",
         "combo_box.item:System Default Language": {
            "@label": {
               "string": "System Default"
            },
            "@name": "System Default Language"
         },
         "combo_box.item:English": {
            "@label": {
               "string": "English"
            },
            "@name": "English"
         },
         "#comment": " After \"System Default\" and \"English\", please keep the rest of these combo_box.items in alphabetical order by the first character in the string. ",
         "combo_box.item:Danish": {
            "@label": {
               "string": "Dansk (Danish)"
            },
            "@name": "Danish"
         },
         "combo_box.item:German": {
            "@label": {
               "string": "Deutsch (German)"
            },
            "@name": "German"
         },
         "combo_box.item:Spanish": {
            "@label": {
               "string": "Español (Spanish)"
            },
            "@name": "Spanish"
         },
         "combo_box.item:French": {
            "@label": {
               "string": "Français (French)"
            },
            "@name": "French"
         },
         "combo_box.item:Italian": {
            "@label": {
               "string": "Italiano (Italian)"
            },
            "@name": "Italian"
         },
         "combo_box.item:Hungarian": {
            "@label": {
               "string": "Magyar (Hungarian)"
            },
            "@name": "Hungarian"
         },
         "combo_box.item:Dutch": {
            "@label": {
               "string": "Nederlands (Dutch)"
            },
            "@name": "Dutch"
         },
         "combo_box.item:Polish": {
            "@label": {
               "string": "Polski (Polish)"
            },
            "@name": "Polish"
         },
         "combo_box.item:Portugese": {
            "@label": {
               "string": "Português (Portuguese)"
            },
            "@name": "Portugese"
         },
         "combo_box.item:Russian": {
            "@label": {
               "string": "Русский (Russian)"
            },
            "@name": "Russian"
         },
         "combo_box.item:Turkish": {
            "@label": {
               "string": "Türkçe (Turkish)"
            },
            "@name": "Turkish"
         },
         "combo_box.item:Ukrainian": {
            "@label": {
               "string": "Українська (Ukrainian)"
            },
            "@name": "Ukrainian"
         },
         "combo_box.item:Chinese": {
            "@label": {
               "string": "中文 (正體) (Chinese)"
            },
            "@name": "Chinese"
         },
         "combo_box.item:Japanese": {
            "@label": {
               "string": "日本語 (Japanese)"
            },
            "@name": "Japanese"
         },
         "combo_box.item:Korean": {
            "@label": {
               "string": "한국어 (Korean)"
            },
            "@name": "Korean"
         }
      },
      "text:tip": {
         "@name": "tip",
         "#text": {
            "string": "\n  Choose translation service:\n "
         }
      },
      "radio_group:translation_service_rg": {
         "@name": "translation_service_rg",
         "radio_item:azure": {
            "@label": {
               "string": "Azure Translator"
            },
            "@name": "azure"
         },
         "radio_item:google": {
            "@label": {
               "string": "Google Translate"
            },
            "@name": "google"
         },
         "radio_item:deepl": {
            "@label": {
               "string": "DeepL Translator"
            },
            "@name": "deepl"
         }
      },
      "text:google_links_text": {
         "@name": "google_links_text",
         "#text": {
            "string": "\n  [http://code.google.com/apis/language/translate/v2/pricing.html Pricing] | [https://code.google.com/apis/console Stats] \n "
         }
      },
      "text:azure_api_endoint_label": {
         "@name": "azure_api_endoint_label",
         "#text": {
            "string": "\n    Endpoint:\n  "
         }
      },
      "combo_box:azure_api_endpoint_combo": {
         "@name": "azure_api_endpoint_combo",
         "@value": {
            "string": "https://api.cognitive.microsofttranslator.com"
         },
         "combo_box.item:global": {
            "@label": {
               "string": "https://api.cognitive.microsofttranslator.com"
            },
            "@name": "global"
         },
         "combo_box.item:api-apc": {
            "@label": {
               "string": "https://api-apc.cognitive.microsofttranslator.com"
            },
            "@name": "api-apc"
         },
         "combo_box.item:api-eur": {
            "@label": {
               "string": "https://api-eur.cognitive.microsofttranslator.com"
            },
            "@name": "api-eur"
         },
         "combo_box.item:api-nam": {
            "@label": {
               "string": "https://api-nam.cognitive.microsofttranslator.com"
            },
            "@name": "api-nam"
         }
      },
      "text:azure_api_key_label": {
         "@name": "azure_api_key_label",
         "#text": {
            "string": "\n  Azure Key:\n  "
         }
      },
      "line_editor:azure_api_key": {
         "@default_text": {
            "string": "Enter Translator Key and click \"Verify\""
         },
         "@name": "azure_api_key"
      },
      "text:azure_api_region_label": {
         "@name": "azure_api_region_label",
         "#text": {
            "string": "\n    Region:\n  "
         }
      },
      "line_editor:azure_api_region": {
         "@default_text": {
            "string": "Can be left empty for global services"
         },
         "@name": "azure_api_region"
      },
      "button:verify_azure_api_key_btn": {
         "@label": {
            "string": "Verify"
         },
         "@name": "verify_azure_api_key_btn"
      },
      "text:google_api_key_label": {
         "@name": "google_api_key_label",
         "#text": {
            "string": "\n  Google [http://code.google.com/apis/language/translate/v2/getting_started.html#auth API key]:\n "
         }
      },
      "line_editor:google_api_key": {
         "@default_text": {
            "string": "Enter Google API key and click \"Verify\""
         },
         "@name": "google_api_key"
      },
      "button:verify_google_api_key_btn": {
         "@label": {
            "string": "Verify"
         },
         "@name": "verify_google_api_key_btn"
      },
      "text:deepl_api_domain_label": {
         "@name": "deepl_api_domain_label",
         "#text": {
            "string": "\n        Endpoint:\n    "
         }
      },
      "combo_box:deepl_api_domain_combo": {
         "@name": "deepl_api_domain_combo",
         "@value": {
            "string": "https://api-free.deepl.com"
         },
         "combo_box.item:global": {
            "@label": {
               "string": "DeepL Free"
            },
            "@name": "global"
         },
         "combo_box.item:api-apc": {
            "@label": {
               "string": "DeepL Pro"
            },
            "@name": "api-apc"
         }
      },
      "text:deepl_api_key_label": {
         "@name": "deepl_api_key_label",
         "#text": {
            "string": "\n      DeepL API key:\n    "
         }
      },
      "line_editor:deepl_api_key": {
         "@default_text": {
            "string": "Enter DeepL API key and click \"Verify\""
         },
         "@name": "deepl_api_key"
      },
      "button:verify_deepl_api_key_btn": {
         "@label": {
            "string": "Verify"
         },
         "@name": "verify_deepl_api_key_btn"
      },
      "text:deepl_links_text": {
         "@name": "deepl_links_text",
         "#text": {
            "string": "\n        [https://www.deepl.com/pro/select-country?cta=header-prices Pricing]\n    "
         }
      },
      "button:ok_btn": {
         "@label": {
            "string": "OK"
         },
         "@name": "ok_btn"
      },
      "button:cancel_btn": {
         "@label": {
            "string": "Cancel"
         },
         "@name": "cancel_btn"
      }
   }
}