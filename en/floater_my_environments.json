                  "@tool_tip": {
                     "string": "More options"
                  }
               },
               "menu_button:btn_newsettings": {
                  "@name": "btn_newsettings",
                  "@tool_tip": {
                     "string": "Make new setting"
                  }
               },
               "button:btn_del": {
                  "@name": "btn_del",
                  "@tool_tip": {
                     "string": "Remove selected item"
                  }
               }
            }
         }
