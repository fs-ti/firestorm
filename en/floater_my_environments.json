{
   "floater": {
      "@name": "my_environments",
      "@title": {
         "string": "My Environments"
      },
      "layout_stack:main_layout": {
         "@name": "main_layout",
         "layout_panel:filter_panel": {
            "@name": "filter_panel",
            "@label": {
               "string": "Filters"
            },
            "icon:icon_settingsdays": {
               "@name": "icon_settingsdays"
            },
            "check_box:chk_days": {
               "@label": {
                  "string": "Days"
               },
               "@name": "chk_days"
            },
            "icon:icon_settingsskies": {
               "@name": "icon_settingsskies"
            },
            "check_box:chk_skies": {
               "@label": {
                  "string": "Skies"
               },
               "@name": "chk_skies"
            },
            "icon:icon_settingswater": {
               "@name": "icon_settingswater"
            },
            "check_box:chk_water": {
               "@label": {
                  "string": "Water"
               },
               "@name": "chk_water"
            },
            "filter_editor:flt_search": {
