{
   "floater": {
      "@name": "group_titles",
      "@title": {
         "string": "Group Titles"
      },
      "floater.string:NoGroupTitle": {
         "@name": "NoGroupTitle",
         "@value": {
            "string": "&lt;No group title&gt;"
         }
      },
      "filter_editor:filter_input": {
         "@label": {
            "string": "Filter Group Titles"
         },
         "@name": "filter_input"
      },
      "scroll_list:title_list": {
         "@name": "title_list",
