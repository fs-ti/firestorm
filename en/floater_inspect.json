{
   "floater": {
      "@name": "inspect",
      "@title": {
         "string": "Inspect Objects"
      },
      "floater.string:timeStamp": {
         "@name": "timeStamp",
         "#text": {
            "string": "\n        [year,datetime,slt] [mth,datetime,slt] [day,datetime,slt], [hour,datetime,slt]:[min,datetime,slt]:[second,datetime,slt], [wkday,datetime,slt]\n    "
         }
      },
      "floater.string:Group": {
         "@name": "Group",
         "#text": {
            "string": "\n        (Group)\n    "
         }
      },
      "floater.string:stats_list": {
         "@name": "stats_list",
         "#text": {
            "string": "\n        Total stats:\n\n[NUM_OBJECTS] objects, [NUM_PRIMS] prims\n\nFaces: [NUM_FACES]\nVisible: [NUM_VISIBLE_FACES]\nVertices: [NUM_VERTICES]\nTriangles: [NUM_TRIANGLES]\nComplex.: [COMPLEXITY]\n\nTextures: [NUM_TEXTURES]\nTMem: [TEXTURE_MEMORY] KB\nVRAM: [VRAM_USAGE] KB\n    "
         }
      },
