{
   "panel": {
      "@name": "panel_notes",
      "@label": {
         "string": "Notes & Privacy"
      },
      "loading_indicator:progress_indicator": {
         "@name": "progress_indicator"
      },
      "text:status_message": {
         "@name": "status_message",
         "@value": {
            "string": "Make notes about this person here. No one else can see your notes."
         }
      },
      "text_editor:notes_edit": {
         "@name": "notes_edit"
      },
      "button:notes_save_changes": {
         "@name": "notes_save_changes",
         "@label": {
            "string": "Save"
         }
      },
      "button:notes_discard_changes": {
         "@name": "notes_discard_changes",
         "@label": {
            "string": "Discard"
         }
      }
   }
}