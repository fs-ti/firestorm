         "#text": {
            "string": "\n        http://secondlife.com/app/tos/\n    "
         }
      },
      "floater.string:loading_url": {
         "@name": "loading_url",
         "#text": {
            "string": "\n      data:text/html;charset=utf-8;base64,\n    "
         }
      },
      "text:tos_heading": {
         "@name": "tos_heading",
