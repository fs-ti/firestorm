{
   "floater": {
      "@name": "region_tracker",
      "@title": {
         "string": "Region Tracker"
      },
      "icon:dummy_icon": {
         "@name": "dummy_icon"
      },
      "text:region_label_text": {
         "@name": "region_label_text",
         "@value": {
            "string": "Add regions from the World Map"
         }
      },
      "button:refresh": {
         "@name": "refresh",
         "@tool_tip": {
            "string": "Refresh"
         }
      },
      "button:remove": {
         "@name": "remove",
         "@tool_tip": {
            "string": "Remove region(s)"
         }
      },
      "button:open_map": {
         "@name": "open_map",
         "@tool_tip": {
            "string": "Open map"
         }
      },
      "scroll_list:region_list": {
         "@name": "region_list",
         "@tool_tip": {
            "string": "Double click an entry to open it on the world map."
         },
         "scroll_list.columns:region_label": {
            "@name": "region_label",
            "@label": {
               "string": "Label"
            }
         },
         "scroll_list.columns:region_maturity_icon": {
            "@name": "region_maturity_icon"
         },
         "scroll_list.columns:region_name": {
            "@name": "region_name",
            "@label": {
               "string": "Region"
            }
         },
         "scroll_list.columns:region_agent_count": {
            "@name": "region_agent_count",
            "@label": {
               "string": "#"
            }
         }
      }
   }
}