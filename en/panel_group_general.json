{
   "panel": {
      "@label": {
         "string": "General"
      },
      "@name": "general_tab",
      "panel.string:help_text": {
         "@name": "help_text",
         "#text": {
            "string": "\n        The General tab contains general information about this group, a list of members, general Group Preferences and member options.\n\nHover your mouse over the options for more help.\n    "
         }
      },
      "panel.string:group_info_unchanged": {
         "@name": "group_info_unchanged",
         "#text": {
            "string": "\n        General group information has changed\n    "
         }
      },
      "panel.string:incomplete_member_data_str": {
         "@name": "incomplete_member_data_str",
         "#text": {
            "string": "\n        Retrieving member data\n    "
         }
      },
      "panel:group_general_panel": {
         "@name": "group_general_panel",
