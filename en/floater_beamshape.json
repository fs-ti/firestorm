{
   "floater": {
      "@name": "BeamCreator",
      "@title": {
         "string": "Beam Shape Creator"
      },
      "text:beamcreator_text": {
         "@name": "beamcreator_text",
         "#text": {
            "string": "\n    To create a new beam, left click the panel below to\ndefine a beam ray of the selected color, or right click to\nremove a ray. Press the Save button when done.\n  "
         }
