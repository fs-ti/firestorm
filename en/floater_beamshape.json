         "@name": "beamshape_load"
      },
      "button:beamshape_clear": {
         "@label": {
            "string": "Clear"
         },
         "@name": "beamshape_clear"
      },
      "button:cancel": {
         "@label": {
            "string": "Cancel"
         },
