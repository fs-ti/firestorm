            "spinner:gamma": {
               "@label": {
                  "string": "Gamma:"
               },
               "@name": "gamma"
            },
            "text:brightness text": {
               "@name": "brightness text",
               "#text": {
                  "string": "\n            (0/1.0 = default brightness, lower = brighter)\n        "
               }
            },
            "slider:GraphicsCardTextureMemory": {
               "@label": {
                  "string": "Viewer Texture Memory Buffer (MB):"
               },
               "@name": "GraphicsCardTextureMemory",
               "@tool_tip": {
                  "string": "Amount of memory to allocate for textures. Defaults to video card memory up to 512MB on 32bit systems and up to 2048MB on 64bit systems, depending on the available dedicated video memory of the video card. Reducing this may improve performance but may also make textures blurry."
               }
            },
            "check_box:FSDynamicTextureMemory": {
               "@label": {
                  "string": "Enable Dynamic Texture Memory"
               },
               "@name": "FSDynamicTextureMemory",
               "@tool_tip": {
                  "string": "Enables dynamic texture memory calculation (only available on 64bit viewers and AMD/NVIDIA GPUs with at least 512MB of video memory)"
               }
            },
            "slider:FSDynamicTextureMemoryMinTextureMemory": {
               "@label": {
                  "string": "Minimum Viewer Texture Memory Buffer (MB):"
               },
               "@name": "FSDynamicTextureMemoryMinTextureMemory",
               "@tool_tip": {
                  "string": "The minimum amount of memory to allocate for textures. This will make sure the specified amount will always be used for textures, even if it exceeds the amount of available video memory. If the specified amount plus the reserves exceed the amount of physical video memory, it will be capped accordingly."
               }
            },
            "slider:FSDynamicTextureMemoryCacheReserve": {
               "@label": {
                  "string": "Additional Texture Memory Cache Reserve (%):"
               },
               "@name": "FSDynamicTextureMemoryCacheReserve",
               "@tool_tip": {
                  "string": "The percentage of physical video memory reserved to cache loaded textures that are currently not being displayed."
               }
            },
            "slider:FSDynamicTextureMemoryGPUReserve": {
               "@label": {
                  "string": "Physical Video Memory Reserve (%):"
               },
               "@name": "FSDynamicTextureMemoryGPUReserve",
               "@tool_tip": {
                  "string": "The percentage of physical video memory reserved for other usage."
               }
            },
            "spinner:fog": {
