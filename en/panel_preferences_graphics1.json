         "#comment$4": "Hardware Settings",
         "panel:Hardware Settings": {
            "@label": {
               "string": "Hardware Settings"
            },
            "@name": "Hardware Settings",
            "check_box:ani": {
               "@label": {
                  "string": "Anisotropic Filtering (slower when enabled)"
               },
               "@name": "ani",
               "@tool_tip": {
                  "string": "This checkbox enables anisotropic filtering, which is a method to enhance the quality of textures when they are viewed at relatively large angles in relation to your camera position. Usually makes them look less blurry at greater distances."
               }
            },
            "check_box:vbo": {
               "@label": {
                  "string": "Enable OpenGL Vertex Buffer Objects"
               },
               "@name": "vbo",
               "@tool_tip": {
                  "string": "Enabling this on modern hardware gives a performance gain.  However, older hardware often has poor implementations of VBOs and you may get crashes when this is enabled."
               }
            },
            "check_box:vbo_stream": {
               "@label": {
                  "string": "Enable Streamed VBOs"
               },
               "@name": "vbo_stream",
               "@tool_tip": {
                  "string": "Disabling this may improve performance when VBOs are enabled. Disabling produced observable improvement on various AMD and Intel GPUs."
               }
            },
            "check_box:texture compression": {
               "@label": {
                  "string": "Enable Lossy Texture Compression (requires restart)"
               },
               "@name": "texture compression",
               "@tool_tip": {
                  "string": "Compresses textures in video memory, allowing for higher resolution textures and/or more textures to be loaded at the cost of some color quality."
               }
            },
            "check_box:use HiDPI": {
               "@label": {
                  "string": "Enable support for HiDPI displays (Mac OSX only; requires restart)"
               },
               "@name": "use HiDPI",
               "@tool_tip": {
                  "string": "Enable OpenGL for High-Resolution Drawing."
               }
            },
            "text:Antialiasing:": {
               "@name": "Antialiasing:",
               "@tool_tip": {
                  "string": "Changes to this setting may require a restart on some hardware."
               },
               "#text": {
                  "string": "\n          Antialiasing (restart recommended):\n        "
               }
            },
            "combo_box:fsaa": {
